{
  "name": "genetic-circuit-simulator",
  "productName": "Genetic Circuit Simulator",
  "version": "1.0.0",
  "description": "Build and simulate biological genetic circuits.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "build": {
    "appId": "genetic-circuit-simulator",
    "productName": "Genetic Circuit Simulator",
    "extraResources": [
      {
        "from": "backend/dist/app",
        "to": "backend"
      }
    ],
    "files": [
      "package.json",
      ".webpack/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "TODO:change this x.app.category"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dom-to-image": "^2.6.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "concurrently": "^9.1.2",
    "css-loader": "^6.11.0",
    "electron": "^34.0.2",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4"
  },
  "keywords": [],
  "author": {
    "name": "Tammy Truong",
    "email": "tammytru000@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@radix-ui/themes": "^3.2.1",
    "@xyflow/react": "^12.4.2",
    "axios": "^1.7.9",
    "dom-to-image": "^2.6.0",
    "electron-forge-maker-appimage": "^25.1.8",
    "electron-squirrel-startup": "^1.0.1",
    "jsonschema": "^1.5.0",
    "lucide-react": "^0.475.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-resizable-panels": "^2.1.7",
    "react-rnd": "^10.5.2",
    "react-select": "^5.10.1"
  }
}
